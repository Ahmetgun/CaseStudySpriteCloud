{
  "name": "casestudyspritecloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:open": "cypress open",
    "create:results": "mkdir cypress/reports/ && mkdir cypress/reports/mochawesome",
    "pretest": "npm run create:results",
    "combine:reports": "npx mochawesome-merge cypress/results/mocha/*.json > cypress/results/mochawesome/finalresult.json",
    "generate:reports": "marge cypress/results/mochawesome/finalresult.json -f report -o cypress/results/mochawesome"
  },
  "author": "ahmet",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.1.1",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1"
  },
  "dependencies": {}
}


"create:reportsfolder": "mkdir cypress/reports/ && mkdir cypress/reports/mochareports",
    "pretest": "npm run create:reportsfolder",
    "clean:screenshots": "rm -R -f cypress/screenshots",
    "combine:reports": "npx mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate:report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",